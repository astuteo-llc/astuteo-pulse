{
    "name": "astuteo/astuteo-pulse",
    "description": "Connecting Astuteo client sites to our monitor.",
    "type": "craft-plugin",
    "prefer-stable": true,
    "version": "4.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "astuteo pulse"
    ],
    "support": {
        "docs": "https://github.com/astuteollc/astuteo-pulse/blob/master/README.md",
        "issues": "https://github.com/astuteollc/astuteo-pulse/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Astuteo",
            "homepage": "https://astuteo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "php": "^8.0.2",
        "ext-json": "*"
    },
    "require-dev": {
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "astuteo\\astuteopulse\\": "src/"
        }
    },
    "extra": {
        "name": "Astuteo Pulse",
        "handle": "astuteo-pulse",
        "developer": "Astuteo",
        "developerUrl": "https://astuteo.com",
        "documentationUrl": "https://github.com/astuteollc/astuteo-pulse/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/astuteollc/astuteo-pulse/master/CHANGELOG.md",
        "class": "astuteo\\astuteopulse\\AstuteoPulse"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
